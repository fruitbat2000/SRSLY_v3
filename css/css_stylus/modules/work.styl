.folio-list
	clearfix();
	margin-top:25px;
	.column_4
		width:399px;
	li
		float:left;
		padding:25px 0;
		border-bottom:dashed 1px #333;
		position:relative;
		&:hover
			background:#098c7f;
			img
				box-shadow:5px 5px 5px #333;
		&:nth-child(3n-1)
			border-left:dashed 1px #333;
			border-right:dashed 1px #333;
		/*&:nth-last-child(-n+3)
			border-bottom:none;*/
		&:last-child
			border-right:dashed 1px #333;
			border-bottom:none;
	img
		display:block;
		width:300px;
		margin:0 auto;
		box-shadow:5px 5px 5px #999;
		cursor:pointer;
#client-list
	text-align:center
	background:#333333;
	color:#f2f1d4;
	font-family:"Quan-RoundedBold", Arial, sans-serif;
	padding:15px;
	border-radius:7px;
	margin:100px 0 50px 0;
	& h2, h3
		margin:0;